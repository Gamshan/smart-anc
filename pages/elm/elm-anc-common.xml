   <annotation startLine="80" startChar="3" endLine="80" endChar="60" message="Could not resolve membership operator for terminology target of the retrieve." errorType="semantic" errorSeverity="warning" xsi:type="a:CqlToElmError"/>
   <identifier id="ANCCommon" version="0.1.0"/>
   <schemaIdentifier id="urn:hl7-org:elm" version="r1"/>
   <usings>
      <def localIdentifier="System" uri="urn:hl7-org:elm-types:r1"/>
      <def localIdentifier="FHIR" uri="http://hl7.org/fhir" version="4.0.0"/>
   </usings>
   <includes>
      <def localIdentifier="FHIRHelpers" path="FHIRHelpers" version="4.0.0"/>
   </includes>
   <codeSystems>
      <def name="SNOMEDCT" id="http://snomed.info/sct" accessLevel="Public"/>
      <def name="ANCCodes" id="http://fhir.org/guides/who/anc-cds/CodeSystem/anc-codes" accessLevel="Public"/>
      <def name="OpenMRSEntity" id="http://openmrs.org/concepts" accessLevel="Public"/>
      <def name="EpisodeOfCareType" id="http://terminology.hl7.org/CodeSystem/episodeofcare-type" accessLevel="Public"/>
   </codeSystems>
   <valueSets>
      <def name="Active Condition" id="http://hl7.org/fhir/uv/cpg/ValueSet/cpg-active-condition" accessLevel="Public"/>
   </valueSets>
   <codes>
      <def name="ANC Pregnancy Case" id="hacc" accessLevel="Public">
         <codeSystem name="EpisodeOfCareType"/>
      </def>
      <def name="LMP" id="1427AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" display="Date of last menstrual period" accessLevel="Public">
         <codeSystem name="OpenMRSEntity"/>
      </def>
      <def name="UltrasoundGA" id="165220AAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" display="Gestational age in weeks from ultrasound" accessLevel="Public">
         <codeSystem name="OpenMRSEntity"/>
      </def>
      <def name="FundalHeight" id="1439AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" display="FUNDAL HEIGHT" accessLevel="Public">
         <codeSystem name="OpenMRSEntity"/>
      </def>
      <def name="Weeks of gestational age" id="1438AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" display="Weeks of gestational age" accessLevel="Public">
         <codeSystem name="OpenMRSEntity"/>
      </def>
      <def name="Pregnancy (finding)" id="TBD" accessLevel="Public">
         <codeSystem name="SNOMEDCT"/>
      </def>
      <def name="Ultrasound (procedure)" id="TBD" accessLevel="Public">
         <codeSystem name="SNOMEDCT"/>
      </def>
      <def name="Selected Gestational Age Method" id="GestAgeMethod" accessLevel="Public">
         <codeSystem name="OpenMRSEntity"/>
      </def>
      <def name="UseLMP" id="UseLMP" accessLevel="Public">
         <codeSystem name="ANCCodes"/>
      </def>
      <def name="UseUltrasound" id="UseUltrasound" accessLevel="Public">
         <codeSystem name="ANCCodes"/>
      </def>
      <def name="UseSFH" id="UseSFH" accessLevel="Public">
         <codeSystem name="ANCCodes"/>
      </def>
   </codes>
   <statements>
      <def name="Patient" context="Patient">
         <expression xsi:type="SingletonFrom">
            <operand dataType="fhir:Patient" xsi:type="Retrieve"/>
         </expression>
      </def>
      <def name="pregnancy" context="Patient" accessLevel="Public">
         <expression xsi:type="First">
            <source xsi:type="Query">
               <source alias="E">
                  <expression dataType="fhir:EpisodeOfCare" codeProperty="type" xsi:type="Retrieve">
                     <codes xsi:type="ToList">
                        <operand name="ANC Pregnancy Case" xsi:type="CodeRef"/>
                     </codes>
                  </expression>
               </source>
               <sort>
                  <by direction="descending" xsi:type="ByExpression">
                     <expression xsi:type="Start">
                        <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand name="period" xsi:type="IdentifierRef"/>
                        </operand>
                     </expression>
                  </by>
               </sort>
            </source>
         </expression>
      </def>
      <def name="GetId" context="Patient" accessLevel="Public" xsi:type="FunctionDef">
         <expression xsi:type="Last">
            <source xsi:type="Split">
               <stringToSplit name="reference" xsi:type="OperandRef"/>
               <separator valueType="t:String" value="/" xsi:type="Literal"/>
            </source>
         </expression>
         <operand name="reference">
            <operandTypeSpecifier name="t:String" xsi:type="NamedTypeSpecifier"/>
         </operand>
      </def>
      <def name="pregnancyCondition" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="C">
               <expression dataType="fhir:Condition" codeProperty="code" xsi:type="Retrieve">
                  <codes xsi:type="ToList">
                     <operand name="Pregnancy (finding)" xsi:type="CodeRef"/>
                  </codes>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="id" scope="C" xsi:type="Property"/>
                  </operand>
                  <operand name="GetId" xsi:type="FunctionRef">
                     <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="reference" xsi:type="Property">
                           <source path="condition" xsi:type="Property">
                              <source xsi:type="First">
                                 <source path="diagnosis" xsi:type="Property">
                                    <source name="pregnancy" xsi:type="ExpressionRef"/>
                                 </source>
                              </source>
                           </source>
                        </operand>
                     </operand>
                  </operand>
               </operand>
               <operand xsi:type="InValueSet">
                  <code name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="clinicalStatus" scope="C" xsi:type="Property"/>
                  </code>
                  <valueset name="Active Condition"/>
               </operand>
            </where>
         </expression>
      </def>
      <def name="lmp" context="Patient" accessLevel="Public">
         <expression xsi:type="First">
            <source xsi:type="Query">
               <source alias="O">
                  <expression dataType="fhir:Observation" codeProperty="code" xsi:type="Retrieve">
                     <codes xsi:type="ToList">
                        <operand name="LMP" xsi:type="CodeRef"/>
                     </codes>
                  </expression>
               </source>
               <where xsi:type="And">
                  <operand xsi:type="Equal">
                     <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="status" scope="O" xsi:type="Property"/>
                     </operand>
                     <operand valueType="t:String" value="final" xsi:type="Literal"/>
                  </operand>
                  <operand xsi:type="SameOrAfter">
                     <operand name="ToDateTime" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand path="issued" scope="O" xsi:type="Property"/>
                     </operand>
                     <operand xsi:type="Start">
                        <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                           <operand path="period" xsi:type="Property">
                              <source name="pregnancy" xsi:type="ExpressionRef"/>
                           </operand>
                        </operand>
                     </operand>
                  </operand>
               </where>
               <return>
                  <expression name="ToDateTime" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand asType="fhir:dateTime" xsi:type="As">
                        <operand path="value" scope="O" xsi:type="Property"/>
                     </operand>
                  </expression>
               </return>
               <sort>
                  <by direction="desc" xsi:type="ByDirection"/>
               </sort>
            </source>
         </expression>
      </def>
      <def name="lmp_known" context="Patient" accessLevel="Public">
         <expression xsi:type="Not">
            <operand xsi:type="IsNull">
               <operand name="lmp" xsi:type="ExpressionRef"/>
            </operand>
         </expression>
      </def>
      <def name="lmp_edd" context="Patient" accessLevel="Public">
         <expression xsi:type="Add">
            <operand xsi:type="Add">
               <operand name="lmp" xsi:type="ExpressionRef"/>
               <operand value="7" unit="days" xsi:type="Quantity"/>
            </operand>
            <operand value="9" unit="months" xsi:type="Quantity"/>
         </expression>
      </def>
      <def name="lmp_gest_age_weeks" context="Patient" accessLevel="Public">
         <expression xsi:type="TruncatedDivide">
            <operand precision="Day" xsi:type="DurationBetween">
               <operand name="lmp" xsi:type="ExpressionRef"/>
               <operand xsi:type="ToDateTime">
                  <operand xsi:type="Today"/>
               </operand>
            </operand>
            <operand valueType="t:Integer" value="7" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="lmp_gest_age_remainder" context="Patient" accessLevel="Public">
         <expression xsi:type="Modulo">
            <operand precision="Day" xsi:type="DurationBetween">
               <operand name="lmp" xsi:type="ExpressionRef"/>
               <operand xsi:type="ToDateTime">
                  <operand xsi:type="Today"/>
               </operand>
            </operand>
            <operand valueType="t:Integer" value="7" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="ultrasoundProcedures" context="Patient" accessLevel="Public">
         <expression xsi:type="Query">
            <source alias="P">
               <expression dataType="fhir:Procedure" codeProperty="code" xsi:type="Retrieve">
                  <codes xsi:type="ToList">
                     <operand name="Ultrasound (procedure)" xsi:type="CodeRef"/>
                  </codes>
               </expression>
            </source>
            <where xsi:type="And">
               <operand xsi:type="Equal">
                  <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="status" scope="P" xsi:type="Property"/>
                  </operand>
                  <operand valueType="t:String" value="completed" xsi:type="Literal"/>
               </operand>
               <operand xsi:type="IncludedIn">
                  <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand asType="fhir:Period" xsi:type="As">
                        <operand path="performed" scope="P" xsi:type="Property"/>
                     </operand>
                  </operand>
                  <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                     <operand path="period" xsi:type="Property">
                        <source name="pregnancy" xsi:type="ExpressionRef"/>
                     </operand>
                  </operand>
               </operand>
            </where>
         </expression>
      </def>
      <def name="select_gest_age_edd" context="Patient" accessLevel="Public">
         <expression strict="false" xsi:type="As">
            <operand path="value" xsi:type="Property">
               <source xsi:type="First">
                  <source xsi:type="Query">
                     <source alias="O">
                        <expression dataType="fhir:Observation" codeProperty="code" xsi:type="Retrieve">
                           <codes xsi:type="ToList">
                              <operand name="Selected Gestational Age Method" xsi:type="CodeRef"/>
                           </codes>
                        </expression>
                     </source>
                     <where xsi:type="And">
                        <operand xsi:type="Equal">
                           <operand name="ToString" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="status" scope="O" xsi:type="Property"/>
                           </operand>
                           <operand valueType="t:String" value="final" xsi:type="Literal"/>
                        </operand>
                        <operand xsi:type="SameOrAfter">
                           <operand name="ToDateTime" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                              <operand path="issued" scope="O" xsi:type="Property"/>
                           </operand>
                           <operand xsi:type="Start">
                              <operand name="ToInterval" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                                 <operand path="period" xsi:type="Property">
                                    <source name="pregnancy" xsi:type="ExpressionRef"/>
                                 </operand>
                              </operand>
                           </operand>
                        </operand>
                     </where>
                     <sort>
                        <by direction="desc" path="issued" xsi:type="ByColumn"/>
                     </sort>
                  </source>
               </source>
            </operand>
            <asTypeSpecifier name="fhir:CodeableConcept" xsi:type="NamedTypeSpecifier"/>
         </expression>
      </def>
      <def name="ultrasound_gest_age_weeks" context="Patient" accessLevel="Public">
         <expression strict="false" xsi:type="As">
            <operand xsi:type="Null"/>
            <asTypeSpecifier name="t:Integer" xsi:type="NamedTypeSpecifier"/>
         </expression>
      </def>
      <def name="ultrasound_edd" context="Patient" accessLevel="Public">
         <expression strict="false" xsi:type="As">
            <operand xsi:type="Null"/>
            <asTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </expression>
      </def>
      <def name="sfh_gest_age_weeks" context="Patient" accessLevel="Public">
         <expression strict="false" xsi:type="As">
            <operand xsi:type="Null"/>
            <asTypeSpecifier name="t:Integer" xsi:type="NamedTypeSpecifier"/>
         </expression>
      </def>
      <def name="sfh_edd" context="Patient" accessLevel="Public">
         <expression strict="false" xsi:type="As">
            <operand xsi:type="Null"/>
            <asTypeSpecifier name="t:DateTime" xsi:type="NamedTypeSpecifier"/>
         </expression>
      </def>
      <def name="gest_age" context="Patient" accessLevel="Public">
         <expression xsi:type="Case">
            <caseItem>
               <when xsi:type="In">
                  <operand name="UseLMP" xsi:type="CodeRef"/>
                  <operand path="codes" xsi:type="Property">
                     <source name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand name="select_gest_age_edd" xsi:type="ExpressionRef"/>
                     </source>
                  </operand>
               </when>
               <then name="lmp_gest_age_weeks" xsi:type="ExpressionRef"/>
            </caseItem>
            <caseItem>
               <when xsi:type="In">
                  <operand name="UseUltrasound" xsi:type="CodeRef"/>
                  <operand path="codes" xsi:type="Property">
                     <source name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand name="select_gest_age_edd" xsi:type="ExpressionRef"/>
                     </source>
                  </operand>
               </when>
               <then name="ultrasound_gest_age_weeks" xsi:type="ExpressionRef"/>
            </caseItem>
            <caseItem>
               <when xsi:type="In">
                  <operand name="UseSFH" xsi:type="CodeRef"/>
                  <operand path="codes" xsi:type="Property">
                     <source name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
                        <operand name="select_gest_age_edd" xsi:type="ExpressionRef"/>
                     </source>
                  </operand>
               </when>
               <then name="sfh_gest_age_weeks" xsi:type="ExpressionRef"/>
            </caseItem>
            <else asType="t:Integer" xsi:type="As">
               <operand xsi:type="Null"/>
            </else>
         </expression>
      </def>
      <def name="edd" context="Patient" accessLevel="Public">
         <expression xsi:type="Case">
            <comparand name="ToConcept" libraryName="FHIRHelpers" xsi:type="FunctionRef">
               <operand name="select_gest_age_edd" xsi:type="ExpressionRef"/>
            </comparand>
            <caseItem>
               <when xsi:type="ToConcept">
                  <operand name="UseLMP" xsi:type="CodeRef"/>
               </when>
               <then name="lmp_edd" xsi:type="ExpressionRef"/>
            </caseItem>
            <caseItem>
               <when xsi:type="ToConcept">
                  <operand name="UseUltrasound" xsi:type="CodeRef"/>
               </when>
               <then name="ultrasound_edd" xsi:type="ExpressionRef"/>
            </caseItem>
            <caseItem>
               <when xsi:type="ToConcept">
                  <operand name="UseSFH" xsi:type="CodeRef"/>
               </when>
               <then name="sfh_edd" xsi:type="ExpressionRef"/>
            </caseItem>
            <else asType="t:DateTime" xsi:type="As">
               <operand xsi:type="Null"/>
            </else>
         </expression>
      </def>
      <def name="Up to 12 Weeks" context="Patient" accessLevel="Public">
         <expression xsi:type="LessOrEqual">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="12" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="20 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="20" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="26 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="26" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="30 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="30" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="34 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="34" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="36 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="36" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="38 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="38" xsi:type="Literal"/>
         </expression>
      </def>
      <def name="40 weeks gestation" context="Patient" accessLevel="Public">
         <expression xsi:type="Equal">
            <operand name="gest_age" xsi:type="ExpressionRef"/>
            <operand valueType="t:Integer" value="40" xsi:type="Literal"/>
         </expression>
      </def>
   </statements>
</library>
